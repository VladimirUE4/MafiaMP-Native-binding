    {
      "id": "view",
      "name": "View",
      "description": "Web view system for creating browser-based UI interfaces. Uses Ultralight rendering engine.",
      "type": "class",
      "functions": [
        {
          "name": "View.create",
          "syntax": "View.create(url, width, height, x, y)",
          "description": "Creates a new web view window",
          "parameters": [
            {"name": "url", "type": "string", "description": "URL to load (http://, https://, or file://)"},
            {"name": "width", "type": "number", "description": "View width in pixels (optional, default: 0 = fullscreen)"},
            {"name": "height", "type": "number", "description": "View height in pixels (optional, default: 0 = fullscreen)"},
            {"name": "x", "type": "number", "description": "X position (optional, default: 0)"},
            {"name": "y", "type": "number", "description": "Y position (optional, default: 0)"}
          ],
          "returns": {"type": "View", "description": "Created view object"},
          "example": "-- Create a fullscreen view\nlocal myView = View.create('https://example.com')\n\n-- Create a positioned window\nlocal hud = View.create('file:///ui/hud.html', 800, 600, 100, 100)"
        },
        {
          "name": "view:getId",
          "syntax": "view:getId()",
          "description": "Gets the view's unique ID",
          "parameters": [],
          "returns": {"type": "number"},
          "example": "local id = myView:getId()\nConsole.log('View ID:', id)"
        },
        {
          "name": "view:setPosition",
          "syntax": "view:setPosition(x, y)",
          "description": "Sets view position on screen",
          "parameters": [
            {"name": "x", "type": "number", "description": "X coordinate"},
            {"name": "y", "type": "number", "description": "Y coordinate"}
          ],
          "returns": {"type": "void"},
          "example": "myView:setPosition(100, 50)"
        },
        {
          "name": "view:getZIndex",
          "syntax": "view:getZIndex()",
          "description": "Gets view z-index (layer order)",
          "parameters": [],
          "returns": {"type": "number"},
          "example": "local z = myView:getZIndex()"
        },
        {
          "name": "view:setZIndex",
          "syntax": "view:setZIndex(zIndex)",
          "description": "Sets view z-index (higher = on top)",
          "parameters": [
            {"name": "zIndex", "type": "number", "description": "Z-index value"}
          ],
          "returns": {"type": "void"},
          "example": "myView:setZIndex(100) -- Bring to front"
        },
        {
          "name": "view:setFocus",
          "syntax": "view:setFocus(focused)",
          "description": "Sets view focus state",
          "parameters": [
            {"name": "focused", "type": "boolean", "description": "Focus state"}
          ],
          "returns": {"type": "void"},
          "example": "myView:setFocus(true) -- Give focus"
        },
        {
          "name": "view:getFocus",
          "syntax": "view:getFocus()",
          "description": "Checks if view has focus",
          "parameters": [],
          "returns": {"type": "boolean"},
          "example": "if myView:getFocus() then\n  Console.log('View is focused')\nend"
        },
        {
          "name": "view:setDisplay",
          "syntax": "view:setDisplay(visible)",
          "description": "Sets view visibility",
          "parameters": [
            {"name": "visible", "type": "boolean", "description": "Visibility state"}
          ],
          "returns": {"type": "void"},
          "example": "myView:setDisplay(true) -- Show view"
        },
        {
          "name": "view:getDisplay",
          "syntax": "view:getDisplay()",
          "description": "Checks if view is visible",
          "parameters": [],
          "returns": {"type": "boolean"},
          "example": "if myView:getDisplay() then\n  Console.log('View is visible')\nend"
        },
        {
          "name": "view:on",
          "syntax": "view:on(eventName, callback)",
          "description": "Listens to events emitted from JavaScript (use emit() in JS)",
          "parameters": [
            {"name": "eventName", "type": "string", "description": "Event name"},
            {"name": "callback", "type": "function", "description": "Callback function receiving event data"}
          ],
          "returns": {"type": "void"},
          "example": "-- Lua side\nmyView:on('buttonClicked', function(data)\n  Console.log('Received from JS:', data.buttonId)\nend)\n\n-- JavaScript side (in the HTML)\n// emit('buttonClicked', {buttonId: 5})"
        },
        {
          "name": "view:emit",
          "syntax": "view:emit(eventName, data)",
          "description": "Sends event to JavaScript (use window.on() in JS to listen)",
          "parameters": [
            {"name": "eventName", "type": "string", "description": "Event name"},
            {"name": "data", "type": "table|string", "description": "Data to send (will be JSON encoded)"}
