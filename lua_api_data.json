{
  "title": "MafiaMP Lua API Reference",
  "version": "1.0.0",
  "categories": [
    {
      "id": "event",
      "name": "Event",
      "description": "Event management system for listening to game and custom events",
      "type": "namespace",
      "functions": [
        {
          "name": "Event.on",
          "syntax": "Event.on(eventName, callback)",
          "description": "Registers a callback for an event",
          "parameters": [
            {"name": "eventName", "type": "string", "description": "Name of the event to listen to"},
            {"name": "callback", "type": "function", "description": "Function to execute when event fires"}
          ],
          "returns": {"type": "void"},
          "example": "Event.on('onPlayerConnected', function(player)\n  Chat.sendToAll(player.name .. ' joined the server!')\nend)"
        },
        {
          "name": "Event.emit",
          "syntax": "Event.emit(eventName, ...)",
          "description": "Triggers a custom event",
          "parameters": [
            {"name": "eventName", "type": "string", "description": "Name of the custom event"},
            {"name": "...", "type": "any", "description": "Arguments to pass to event listeners"}
          ],
          "returns": {"type": "void"},
          "example": "Event.emit('myCustomEvent', 'Hello', 123)\n\nEvent.on('myCustomEvent', function(msg, num)\n  Console.log(msg, num)\nend)"
        }
      ],
      "events": [
        {"name": "onGamemodeLoaded", "params": [], "description": "Called when gamemode is loaded"},
        {"name": "onGamemodeUnloading", "params": [], "description": "Called before gamemode unloads"},
        {"name": "onGamemodeUpdated", "params": [], "description": "Called every frame (use with caution)"},
        {"name": "onPlayerConnected", "params": [{"name": "player", "type": "Player"}], "description": "Player connects to server"},
        {"name": "onPlayerDisconnected", "params": [{"name": "player", "type": "Player"}], "description": "Player disconnects from server"},
        {"name": "onPlayerDied", "params": [{"name": "player", "type": "Player"}], "description": "Player dies"},
        {"name": "onChatMessage", "params": [{"name": "player", "type": "Player"}, {"name": "message", "type": "string"}], "description": "Player sends chat message"},
        {"name": "onChatCommand", "params": [{"name": "player", "type": "Player"}, {"name": "message", "type": "string"}, {"name": "command", "type": "string"}, {"name": "args", "type": "string[]"}], "description": "Player sends command"},
        {"name": "onVehiclePlayerEnter", "params": [{"name": "vehicle", "type": "Vehicle"}, {"name": "player", "type": "Player"}, {"name": "seatIndex", "type": "integer"}], "description": "Player enters vehicle"},
        {"name": "onVehiclePlayerLeave", "params": [{"name": "vehicle", "type": "Vehicle"}, {"name": "player", "type": "Player"}], "description": "Player leaves vehicle"}
      ]
    },
    {
      "id": "world",
      "name": "World",
      "description": "World management - time, weather, entity creation",
      "type": "namespace",
      "functions": [
        {
          "name": "World.createVehicle",
          "syntax": "World.createVehicle(modelName)",
          "description": "Creates a new vehicle in the world",
          "parameters": [
            {"name": "modelName", "type": "string", "description": "Model name of the vehicle (e.g. 'shubert_six')"}
          ],
          "returns": {"type": "Vehicle", "description": "The created vehicle object"},
          "example": "local car = World.createVehicle('shubert_six')\ncar:setPosition(Vector3.new(0, 0, 0))\ncar:setColorPrimary(RGB.new(255, 0, 0))"
        },
        {
          "name": "World.getDayTimeHours",
          "syntax": "World.getDayTimeHours()",
          "description": "Gets current time of day",
          "parameters": [],
          "returns": {"type": "number", "description": "Time in hours (0.0 - 24.0)"},
          "example": "local currentTime = World.getDayTimeHours()\nConsole.log('Current time: ' .. currentTime)"
        },
        {
          "name": "World.setDayTimeHours",
          "syntax": "World.setDayTimeHours(hours)",
          "description": "Sets the time of day",
          "parameters": [
            {"name": "hours", "type": "number", "description": "Time in hours (0.0 - 24.0)"}
          ],
          "returns": {"type": "void"},
          "example": "World.setDayTimeHours(12.0) -- Set to noon"
        },
        {
          "name": "World.getWeatherSet",
          "syntax": "World.getWeatherSet()",
          "description": "Gets current weather set name",
          "parameters": [],
          "returns": {"type": "string", "description": "Weather set name"},
          "example": "local weather = World.getWeatherSet()\nConsole.log('Weather: ' .. weather)"
        },
        {
          "name": "World.setWeatherSet",
          "syntax": "World.setWeatherSet(weatherSetName)",
          "description": "Sets the weather",
          "parameters": [
            {"name": "weatherSetName", "type": "string", "description": "Name of weather set"}
          ],
          "returns": {"type": "void"},
          "example": "World.setWeatherSet('DT_FreeRide_Day')"
        }
      ]
    },
    {
